int array[] = new int[input2];

			array[input2-1] = input1[input2-1];
int res1 = 0, res2 = 0, sum = 0;
	for(int i = input2-2; i>=0; i--)
	{

		array[i] =  input1[i] - array[i+1]; 

	}
	for(int i = 0 ; i < array.length;i++)
		sum += array[i]; 
                                                                                                                                 
			res1 = array[0];
			res2 = sum;

		UserMainCode.Result obj = new Result(res1,res2);

		return obj;
